# Environment variables template for development

# Environment
ENV=development
DEBUG=True

# Paths
DATA_DIR=./data
OUTPUT_DIR=./output
LOGS_DIR=./logs

# Simulation settings
RESOLUTION=1920,1080
FPS=60
QUALITY=high

# Physics simulation
PHYSICS_ENABLED=true
PHYSICS_TIMESTEP=0.016
GRAVITY=9.81

# Rendering
RENDERING_ENABLED=true
RENDER_QUALITY=high
ENABLE_SHADOWS=true
ENABLE_REFLECTIONS=true

# Machine Learning
ML_ENABLED=true
MODEL_PATH=./models

# Quantum
QUANTUM_ENABLED=false
QUANTUM_BACKEND=qiskit

# Web Interface
WEB_HOST=0.0.0.0
WEB_PORT=5000
WEB_DEBUG=true

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s

# GPU
USE_GPU=true
GPU_ID=0

# Database (optional)
# DATABASE_URL=sqlite:///./database.db

# API (optional)
# API_KEY=your-api-key-here
# API_SECRET=your-api-secret-here

# Export
EXPORT_FORMATS=mp4,png,hdr
EXPORT_QUALITY=high
